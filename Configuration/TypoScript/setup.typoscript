module {
    tx_form {
        settings {
            yamlConfigurations {
                500 = EXT:form_custom_templates/Configuration/Yaml/FormSetup.yaml
            }
        }
    }
}

plugin.tx_form {
    view {
        templateRootPaths {
            500 = EXT:form_custom_templates/Resources/Private/Templates/Form/
        }
        partialRootPaths {
            500 = EXT:form_custom_templates/Resources/Private/Partials/Form/
        }
        layoutRootPaths {
            500 = EXT:form_custom_templates/Resources/Private/Layouts/Form/
        }
    }
    settings < module.tx_form.settings
}
